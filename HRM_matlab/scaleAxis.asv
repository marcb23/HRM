function [local_min, local_max] = scaleAxis(pos, data, time)
x = pos
finish = pos;
current_time = time(pos)
while(pos > 1 && time(pos) > current_time - 10)
    pos = pos-1;
end
start = pos;
local_min = data(start);
local_max = data(start);

for i=start:finish
    if(data(i) < local_min)
        local_min = data(i);
    end
    if(data(i) > local_max)
        local_max = data(i);
    end
end

local_min = local_min-100;if
local_max = local_max+100;